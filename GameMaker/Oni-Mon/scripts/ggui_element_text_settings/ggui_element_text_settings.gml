///ggui_element_text_settings(font,color,halign,valign)
function ggui_element_text_settings(argument0, argument1, argument2, argument3) {
	//Sets configurations to use in subsequent ggui_element_text calls.
	ggui_font   = argument0;
	ggui_color  = argument1;
	ggui_halign = argument2;
	ggui_valign = argument3;



}
